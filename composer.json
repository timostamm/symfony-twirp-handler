{
  "name": "timostamm/symfony-twirp-handler",
  "type": "library",
  "description": "Helps implementing Twirp in a Symfony application",
  "license": "MIT",
  "require": {
    "php": ">=7.1",
    "google/protobuf": "^3.10",
    "symfony/property-info": ">=4",
    "symfony/event-dispatcher": ">=4",
    "symfony/http-kernel": ">=4",
    "symfony/http-foundation": ">=4.3",
    "psr/container": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5"
  },
  "suggest": {
    "ext-bcmath": "Need to support JSON deserialization",
    "symfony/http-foundation": "To handle requests",
    "psr/log": "To handle requests",
    "psr/container": "To resolve service implementation from container"
  },
  "autoload": {
    "psr-4": {
      "SymfonyTwirpHandler\\": "src/SymfonyTwirpHandler"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "SymfonyTwirpHandler\\": "tests/SymfonyTwirpHandler",
      "Example\\": "out-php/Example"
    }
  }
}
